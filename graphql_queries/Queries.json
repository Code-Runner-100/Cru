query GetRangeByTopic($start: timestamp!, $end: timestamp!, $topicPrefix: String!) {
  vw_mqtt_parsed(
    where: {
      time: { _gte: $start, _lt: $end }
      topic: { _ilike: $topicPrefix }
    }
    order_by: { time: asc }
  ) {
    topic
    time
    v_array
  }
}


